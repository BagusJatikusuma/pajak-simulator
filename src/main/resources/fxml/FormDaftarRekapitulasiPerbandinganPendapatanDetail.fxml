<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.bekasidev.app.viewfx.javafxapplication.content.pelaksanaan.FormDaftarRekapitulasiPerbandinganPendapatanDetailController">
   <children>
      <Label text="Omzet Hasil Pemeriksaan">
         <VBox.margin>
            <Insets bottom="5.0" />
         </VBox.margin>
      </Label>
       <TextField fx:id="omzetHasilField">
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </TextField>
      <Label text="Omzet Yang Dilaporkan">
         <VBox.margin>
            <Insets bottom="5.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="omzetDiLaporkanField">
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </TextField>
      <HBox alignment="CENTER_RIGHT">
         <children>
            <Button fx:id="cancelBtn" mnemonicParsing="false" onAction="#cancelOperation" text="Batal">
               <padding>
                  <Insets bottom="3.0" left="20.0" right="20.0" top="3.0" />
               </padding>
            </Button>
            
                  <Button mnemonicParsing="false" onAction="#finishOperation" text="Simpan">
               <padding>
                  <Insets bottom="3.0" left="20.0" right="20.0" top="3.0" />
               </padding>
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="14.0" left="14.0" right="14.0" top="14.0" />
   </padding>
</VBox>
