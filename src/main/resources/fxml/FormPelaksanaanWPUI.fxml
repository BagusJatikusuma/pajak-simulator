<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox alignment="TOP_CENTER" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.bekasidev.app.viewfx.javafxapplication.content.pelaksanaan.FormPelaksanaanWPUIController">
   <children>
      <Label text="PILIH WAJIB PAJAK">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </Label>
      <TableView fx:id="pilihWajibPajakTable">
         <columns>
            <TableColumn fx:id="idWP" prefWidth="162.0" text="ID WP" />
            <TableColumn fx:id="namaWP" prefWidth="286.0" text="NAMA WP" />
            <TableColumn fx:id="jenisWP" prefWidth="203.0" text="JENIS WP" />
         </columns>
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </TableView>
      <HBox alignment="CENTER_RIGHT">
         <children>
            <Button fx:id="backBtn" mnemonicParsing="false" onAction="#backToPilihTim" text="Kembali">
               <padding>
                  <Insets bottom="3.0" left="20.0" right="20.0" top="3.0" />
               </padding>
            </Button>
            <Button mnemonicParsing="false" onAction="#openAturRekapitulasi" text="Selanjutnya | Atur Rekapitulasi">
               <padding>
                  <Insets bottom="3.0" left="20.0" right="20.0" top="3.0" />
               </padding>
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="14.0" left="14.0" right="14.0" top="14.0" />
   </padding>
</VBox>
