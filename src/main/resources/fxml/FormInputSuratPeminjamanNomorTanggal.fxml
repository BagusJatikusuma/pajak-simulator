<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox minWidth="-Infinity" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.bekasidev.app.viewfx.javafxapplication.content.persiapan.FormInputSuratPeminjamanNomorTanggalController">
   <children>
      <Label text="Nomor Urut Surat">
         <VBox.margin>
            <Insets bottom="5.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="nomorSuratField" maxWidth="1.7976931348623157E308" minWidth="-Infinity" prefHeight="25.0">
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </TextField>
      <Label text="Tanggal Pengesahan">
         <VBox.margin>
            <Insets bottom="5.0" />
         </VBox.margin>
      </Label>
      <DatePicker fx:id="tanggalPengesahanField" maxWidth="1.7976931348623157E308" minWidth="-Infinity" prefHeight="25.0">
         <VBox.margin>
            <Insets bottom="15.0" />
         </VBox.margin>
      </DatePicker>
      <HBox alignment="CENTER_RIGHT">
         <children>
            <Button fx:id="cancelBtn" minWidth="-Infinity" mnemonicParsing="false" onAction="#cancelOperation" text="Batal">
               <padding>
                  <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
               </padding>
            </Button>
            <Button minWidth="-Infinity" mnemonicParsing="false" onAction="#tambahOperation" text="Tambah">
               <padding>
                  <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
               </padding>
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Button>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </HBox>
   </children>
   <padding>
      <Insets bottom="14.0" left="14.0" right="14.0" top="14.0" />
   </padding>
</VBox>
